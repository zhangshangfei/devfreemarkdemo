<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsf.freemark.devfreemarkdemo.usermanager.dao.UserMapper">

    <select id="findByUserNameAndPwd" resultType="com.zsf.freemark.devfreemarkdemo.usermanager.model.UserModel">
        select * from user where userName = #{userName} and passWord = #{passWord}
    </select>

</mapper>